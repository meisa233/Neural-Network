# Build the left part of the Siamese Network:
layer {
  name: "conv1/7x7_s2_left"
  type: "Convolution"
  bottom: "data_left"
  top: "conv1/7x7_s2_left"
  param {
    name: "conv1/7x7_s2_w"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "conv1/7x7_s2_b"
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    pad: 3
    kernel_size: 7
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0.2
    }
  }
}
layer {
  name: "conv1/relu_7x7_left"
  type: "ReLU"
  bottom: "conv1/7x7_s2_left"
  top: "conv1/7x7_s2_left"
}
layer {
  name: "pool1/3x3_s2_left"
  type: "Pooling"

name: "SiameseNetwork"

# Read data for the left part of the Siamese Network:
layer {
  name: "data_left"
  type: "Data"
  top: "data_left"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {
    mirror: true
    crop_size: 224
    mean_value: 104
    mean_value: 117
    mean_value: 123
    scale: 0.00390625
  }
  data_param {
    source: "/home/jedrzej/siamese_data/siamese_train_left_lmdb"
    batch_size: 50
    backend: LMDB
  }
}
layer {
  name: "data_left"
  type: "Data"
  top: "data_left"
  top: "label"
  include {
    phase: TEST
  }
  transform_param {
    mirror: false
    crop_size: 224
    mean_value: 104
    mean_value: 117
    mean_value: 123
    scale: 0.00390625
  }
  data_param {
    source: "/home/jedrzej/siamese_data/siamese_val_left_lmdb"
    batch_size: 25
    backend: LMDB
  }
}

# Read data for the right part of the Siamese Network:
layer {
  name: "data_right"
  type: "Data"
  top: "data_right"
